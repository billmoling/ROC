@{
    Layout = "~/Views/Shared/NewsLayout.cshtml";
}
<input type="hidden" id="pagename" value="video" />

@section HeadStaff
{
    <script type="text/javascript" src="@Url.Content("~/Scripts/VideoTool/mediaelement-and-player.min.js")"></script>
    <link rel="stylesheet" href="@Url.Content("~/Scripts/VideoTool/mediaelementplayer.css")" />
}
@*<video src="@Url.Content("~/Content/Video/XinJieKou.mp4")" width="320" height="240"></video>*@

<video width="320" height="240" poster="" controls="controls" preload="none">
    <!-- MP4 for Safari, IE9, iPhone, iPad, Android, and Windows Phone 7 -->
    <source type="video/webm" src="@Url.Content("~/Content/Video/XinJieKou.webm")" />
    <!-- Flash fallback for non-HTML5 browsers without JavaScript -->
    <object width="320" height="240" type="application/x-shockwave-flash" data="@Url.Content("~/Scripts/VideoTool/flashmediaelement.swf")">
        <param name="movie" value="@Url.Content("~/Scripts/VideoTool/flashmediaelement.swf")" />
        <param name="flashvars" value="controls=true&file=@Url.Content("~/Content/Video/XinJieKou.webm")" />
        
      
    </object>
</video>